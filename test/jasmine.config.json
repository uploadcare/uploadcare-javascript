{
  "spec_dir": "test",
  "spec_files": ["**/*.test.ts", "**/*.test.node.ts"],
  "stopSpecOnExecutionFailure": true,
  "helpers": [
    "./helpers/**/*.ts"
  ],
  "reporters": [
    {
      "name": "jasmine-spec-reporter#SpecReporter",
      "options": {
        "displayStacktrace": "all"
      }
    }
  ]
}
