{
  "name": "@uploadcare/upload-client",
  "version": "1.1.2",
  "description": "Library for work with Uploadcare Upload API",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/types.d.ts",
  "sideEffects": false,
  "files": [
    "dist/*"
  ],
  "browser": {
    "./dist/index.cjs": "./dist/index.browser.cjs",
    "./dist/index.js": "./dist/index.browser.js"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "check-env-vars": "node ./checkvars.js",
    "mock:start": "node --loader ts-node/esm.mjs ./mock-server/server.ts --silent",
    "clean": "rimraf dist",
    "lint": "eslint ./src ./mock-server --ext=ts",
    "test": "start-server-and-test mock:start :3000 test:jest",
    "test:production": "npm run check-env-vars && TEST_ENV=production jest",
    "test:jest": "jest",
    "prebuild": "npm run clean",
    "build": "npm run build:types && npm run build:compile",
    "build:types": "dts-bundle-generator -o dist/types.d.ts src/index.ts",
    "build:compile": "rollup -c",
    "release:prepare": "shipjs prepare",
    "release:trigger": "shipjs trigger"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/uploadcare/uploadcare-upload-client.git"
  },
  "author": "Uploadcare",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/uploadcare/uploadcare-upload-client/issues"
  },
  "homepage": "https://github.com/uploadcare/uploadcare-upload-client#readme",
  "keywords": [
    "uploadcare",
    "file",
    "uploader",
    "image",
    "upload",
    "store",
    "cdn"
  ],
  "devDependencies": {
    "@koa/cors": "3.1.0",
    "@koa/router": "8.0.8",
    "@rollup/plugin-alias": "^3.1.1",
    "@rollup/plugin-node-resolve": "^8.4.0",
    "@types/form-data": "2.5.0",
    "@types/jest": "26.0.9",
    "@types/koa": "2.11.3",
    "@types/node": "12.12.54",
    "@types/promise": "7.1.30",
    "@types/ws": "7.2.6",
    "@typescript-eslint/eslint-plugin": "2.34.0",
    "@typescript-eslint/parser": "2.34.0",
    "chalk": "4.1.0",
    "data-uri-to-buffer": "3.0.1",
    "dataurl-to-blob": "0.0.1",
    "dotenv": "8.2.0",
    "dts-bundle-generator": "5.4.0",
    "eslint": "7.6.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-prettier": "3.1.4",
    "jest": "26.2.2",
    "jest-environment-jsdom": "26.2.0",
    "jest-websocket-mock": "2.1.0",
    "koa": "2.13.0",
    "koa-add-trailing-slashes": "2.0.1",
    "koa-body": "4.2.0",
    "mock-socket": "9.0.3",
    "prettier": "1.19.1",
    "prettier-config-standard": "1.0.1",
    "rimraf": "3.0.2",
    "rollup": "^2.23.0",
    "rollup-plugin-typescript2": "^0.27.1",
    "shipjs": "0.20.1",
    "start-server-and-test": "1.11.3",
    "ts-jest": "26.1.4",
    "ts-node": "8.10.2",
    "typescript": "^3.9.7"
  },
  "dependencies": {
    "abort-controller": "^3.0.0",
    "form-data": "^3.0.0",
    "ws": "^7.2.1"
  }
}
