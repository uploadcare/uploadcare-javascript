{
  "name": "@uploadcare/upload-client",
  "version": "1.0.0-alpha.5",
  "description": "Library for work with Uploadcare Upload API",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib/**/*"
  ],
  "browser": {
    "./lib/request/request.node.js": "./lib/request/request.browser.js",
    "./lib/tools/getFormData.node.js": "./lib/tools/getFormData.browser.js"
  },
  "scripts": {
    "clean": "rimraf lib",
    "lint": "eslint . --ext=ts",
    "mock:start": "cd ./mock-server && npm run start",
    "test": "npm run test:node",
    "test:node": "jasmine-ts --project test/tsconfig.json --config=test/jasmine.config.json",
    "test:browser": "karma start karma.config.js --single-run --nocache",
    "test:browser:debug": "karma start karma.config.js --browsers=Chrome --single-run=false --watch --debug --nocache",
    "prebuild": "npm run clean",
    "build": "tsc --build tsconfig.build.json",
    "release:prepare": "shipjs prepare",
    "release:trigger": "shipjs trigger"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/uploadcare/uploadcare-upload-client.git"
  },
  "author": "Uploadcare",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/uploadcare/uploadcare-upload-client/issues"
  },
  "homepage": "https://github.com/uploadcare/uploadcare-upload-client#readme",
  "keywords": [
    "uploadcare",
    "file",
    "uploader",
    "image",
    "upload",
    "store",
    "cdn"
  ],
  "devDependencies": {
    "@types/form-data": "^2.5.0",
    "@types/jasmine": "^3.5.0",
    "@types/promise": "^7.1.30",
    "@typescript-eslint/eslint-plugin": "^2.2.0",
    "@typescript-eslint/parser": "^2.2.0",
    "data-uri-to-buffer": "^2.0.1",
    "dataurl-to-blob": "0.0.1",
    "eslint": "^6.3.0",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-prettier": "^3.1.1",
    "jasmine": "^3.4.0",
    "jasmine-core": "^3.4.0",
    "jasmine-spec-reporter": "^4.2.1",
    "jasmine-ts": "^0.3.0",
    "jasmine-ts-console-reporter": "^3.1.1",
    "karma": "^4.3.0",
    "karma-chrome-launcher": "^3.1.0",
    "karma-jasmine": "^2.0.1",
    "karma-typescript": "^4.1.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1",
    "prettier-config-standard": "^1.0.1",
    "puppeteer": "^1.19.0",
    "rimraf": "^3.0.0",
    "rollup": "^1.21.2",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-license": "^0.12.1",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-size-snapshot": "^0.10.0",
    "rollup-plugin-terser": "^5.1.3",
    "rollup-plugin-typescript2": "^0.24.1",
    "shipjs": "0.12.0",
    "ts-node": "^8.3.0",
    "typescript": "^3.6.3"
  },
  "dependencies": {
    "form-data": "^2.5.1"
  }
}
